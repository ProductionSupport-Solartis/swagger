            type: string
          StateCode:
            type: string
          State:
            type: string
          ZipCode:
            type: string
          PhoneNumber:
            type: string
          EmailAddress:
            type: string
          MunicipalityName:
            type: string
          MunicipalityCode:
            type: string
          DoAnyofTheFlowingApply:
            type: string
            enum: ["Yes", "No"]
          DoYouOwnOperateorCharterAnyVessels:
            type: string
            enum: ["Yes", "No"]
          HaveYouHadLiabilityClaimsBroughtAgainstYouInTheLast3Years?:
            type: string
            enum: ["Yes", "No"]
          OrganizationType:
            type: string
          QuoteType:
            type: string
            default: "New Business"
          EventName:
            type: string
            default: "MGLCreateQuote"
          EventVersion:
            type: string
            default: "1.0.0.1"
          Location:
            type: object
            properties:
              LocationDetail:
                type: array
                items:
                  properties:
                    LocationID:
                      type: string
                    AddressLine1:
                      type: string
                    AddressLine2:
                      type: string
                    City:
                      type: string
