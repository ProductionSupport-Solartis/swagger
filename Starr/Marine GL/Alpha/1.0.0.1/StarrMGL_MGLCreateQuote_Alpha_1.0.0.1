swagger: '2.0'
info:
  version: '1.0.0.1'
  title: Marine GL - Quote API
  description: |
    Get Quote for the Marine GL class types. 
  
    Features:
    1. Accept input as JSON request with Class Type, Deductible, Gross Receipts, Insured and location details
    2. Respond back with static information and a static Quote number
    
  contact:
    name: Solartis Support
    email: solartis_assist_support@solartis.com

host: starrassistuatkb.uat.solartis.net
basePath: /DroolsV4_2/DroolsService
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Create Quote for the given input
      operationId: createQuote
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "MGLCreateQuote"
        - name: EventVersion
          in: header
          description: Name of the version to be invoked
          required: true
          type: string
          format: string
          default: '1.0.0.1'
        - name: Quote
          in: body
          description: Quoting content
          required: true
          schema:
            $ref: '#/definitions/Quote_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Quote_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
definitions:
  Quote_Request:
    type: object
    required:
     - ServiceRequestDetail
     - QuoteInformation
    properties:
      EndClientUserUniqueSessionId:
        type: string
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        required:
          - OwnerId
          - RegionCode
          - Token
          - ServiceRequestVersion
          - ServiceResponseVersion
          - ResponseType
        properties:
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          OwnerId:
            type: string
            default: "34"
          ResponseType:
            type: string
            default: "JSON"
          RegionCode:
            type: string
            default: "US"
          Token:
            type: string
            default: ""
          UserName:
            type: string
            default: "travelagent"
          LanguageCode:
            type: string
            default: "en"
          BrowserIp:
            type: string
      QuoteInformation:
        type: object
        required:
          - ProducerCode
          - OwnerId
          - StateCode
          - EventName
          - EventVersion
          - ClassType
          - Deductible
          - AnnualGrossReceipts
          - ProductNumber
          - ProductVerNumber
          - Location
        properties:
          ProductNumber:
            type: string
            default: "MGL"
          ProductVerNumber:
            type: string
            default: "1.0"
          ProducerCode:
            type: string
            default: "1558"
          OwnerId:
            type: string
            default: "34"
          ClassType:
            type: string
            enum: ["Small Marine","Artisan Contractor"]
          LimiteachOccurrenceincludingSupplementaryPayments:
            type: string
            default: "1,000,000 CSL"
          ProductsHazardorCompletedOperations:
            type: string
            default: "2,000,000"
          PersonalInjuryandAdvertisingInjuryLimit:
            type: string
            default: "1,000,000"
          DamagetoPremisesRentedtoyouLimit:
            type: string
            default: "100,000 ANY ONE OCCURRENCE"
          PremisesMedicalPaymentsLimit:
            type: string
            default: "10,000 ANY ONE PERSON"
          GeneralAggregateLimitOtherThanProductsCompletedOperations:
            type: string
            default: "2,000,000"
          PolicyEffectiveDate:
            type: string
          Deductible:
            type: string
            enum: ["1500", "2500"]
          AnnualGrossReceipts:
            type: string
          IsTRIAApplicable:
            type: string
            enum: ["Yes", "No"]
          InsuredFirstName:
            type: string
          InsuredLastName:
            type: string
          AddressLine1:
            type: string
          AddressLine2:
            type: string
          City:
            type: string
          County:
            type: string
          StateCode:
            type: string
          State:
            type: string
          ZipCode:
            type: string
          PhoneNumber:
            type: string
          EmailAddress:
            type: string
          MunicipalityName:
            type: string
          MunicipalityCode:
            type: string
          DoAnyofTheFlowingApply:
            type: string
            enum: ["Yes", "No"]
          DoYouOwnOperateorCharterAnyVessels:
            type: string
            enum: ["Yes", "No"]
          HaveYouHadLiabilityClaimsBroughtAgainstYouInTheLast3Years?:
            type: string
            enum: ["Yes", "No"]
          OrganizationType:
            type: string
          QuoteType:
            type: string
            default: "New Business"
          EventName:
            type: string
            default: "MGLCreateQuote"
          EventVersion:
            type: string
            default: "1.0.0.1"
          Location:
            type: object
            properties:
              LocationDetail:
                type: array
                items:
                  properties:
                    LocationID:
                      type: string
                    AddressLine1:
                      type: string
                    AddressLine2:
                      type: string
                    City:
                      type: string
                    County:
                      type: string
                    State:
                      type: string
                    ZipCode:
                      type: string
                    Classification:
                      type: object
                      properties:
                        ClassificationDetail:
                          type: array
                          items:
                            properties:
                              ClassCode:
                                type: string
                                default: "1629"
                  
  Quote_Success_Response:
    type: object
    required:
      - RequestStatus
      - PremiumInformation
    properties:
      messageDetail:
        type: object
      RequestStatus:
        type: string
      PremiumInformation:
        type: object
        properties:
          ProducerCode:
            type: string
            default: "1558"
          OwnerId:
            type: string
            default: "34"
          ClassType:
            type: string
            enum: ["Small Marine","Artisan Contractor"]
          QuoteNumber:
            type: string
          LimiteachOccurrenceincludingSupplementaryPayments:
            type: string
            default: "1,000,000 CSL"
          ProductsHazardorCompletedOperationsHazardorwithrespecttoliabilityarisingoutofindependentcontractorsAggregateLimit:
            type: string
            default: "2,000,000"
