          PremisesMedicalPaymentsLimit:
            type: string
            default: "10,000 ANY ONE PERSON"
          GeneralAggregateLimitOtherThanProductsCompletedOperations:
            type: string
            default: "2,000,000"
          PolicyEffectiveDate:
            type: string
          Deductible:
            type: string
            enum: ["1500", "2500"]
          AnnualGrossReceipts:
            type: string
          IsTRIAApplicable:
            type: string
            enum: ["Yes", "No"]
          InsuredFirstName:
            type: string
