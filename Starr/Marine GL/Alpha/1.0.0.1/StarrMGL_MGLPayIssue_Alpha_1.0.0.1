swagger: '2.0'
info:
  version: '1.0.0.1'
  title: Marine GL - Pay Issue API
  description: |
    Pay and Issue API accepts the Quote Number as input along with
    Payment information and issues a Policy with corresponding documents.
  
    Features:
    1. Provides ability to Pay for a Quote created and provides a static response with a sample policy
    
  contact:
    name: Solartis Support
    email: solartis_solartis_support@solartis.com

host: starrassistuatkb.uat.solartis.net
basePath: /DroolsV4_2/DroolsService
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: This API issues the Policy from the Quote
      operationId: Pay_Issue
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: >-
            
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: MGL_PayIssue
        - name: EventVersion
          in: header
          description: Name of the version to be invoked
          required: true
          type: string
          format: string
          default: '1.0.0.1'
        - name: Pay_Issue
          in: body
          description: Pay_Issue/traveler details
          required: true
          schema:
            $ref: '#/definitions/Pay_Issue_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Pay_Issue_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
definitions:
  Pay_Issue_Request:
    type: object
    required:
      - ServiceRequestDetail
      - PolicyInformation
    properties:
      EndClientUserUniqueSessionId:
        type: string
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        required:
          - OwnerId
          - RegionCode
          - Token
          - ServiceRequestVersion
          - ServiceResponseVersion
          - ResponseType
        properties:
          ServiceRequestVersion:
            type: string
            default: '1.0'
          ServiceResponseVersion:
            type: string
            default: '1.0'
          OwnerId:
            type: string
            default: '34'
            enum:
              - '34'
          ResponseType:
            type: string
            enum:
              - JSON
            default: JSON
          RegionCode:
            type: string
            default: US
          Token:
            type: string
            default: >-
              
          UserName:
            type: string
            default: MGLAgent
          LanguageCode:
            type: string
            default: en
          BrowserIp:
            type: string
      PolicyInformation:
        type: object
        required:
          - ProducerCode
          - ProductNumber
          - ProductVerNumber
          - OwnerId
        properties:
          ProductNumber:
            type: string
            default: MGL
          ProductVerNumber:
            type: string
            default: '1.0'
          ProducerCode:
            type: string
            default: '5'
          OwnerId:
            type: string
            default: '34'
            enum:
              - '34'
          QuoteNumber:
            type: string
          EventName:
            type: string
            default: MGL_PayIssue
            enum:
              - MGL_PayIssue
          EventVersion:
            type: string
            default: '1.0.0.1'
          CardNumber:
            type: string
            enum:
              - '5555555555554444'
              - '4012888888881881'
              - '6011000990139424'
              - '3530111333300000'
              - '347712669659320'
          CVV:
            type: string
            default: '123'
          ExpiryMonth:
            type: string
          ExpiryYear:
            type: string
          PayerName:
            type: string
          PayerAddress1:
            type: string
          PayerAddress2:
            type: string
          PayerCity:
            type: string
          PayerState:
            type: string
            description: Provide Payer State Code
          PayerCountry:
            type: string
            default: US
          PayerZipcode:
            type: string
          PayerEmail:
            type: string
          PayerPhone:
            type: string
          PaymentMethod:
            type: string
            enum:
              - Credit Card
          CardType:
            type: string
            enum:
              - MasterCard
              - Visa
              - JCB
              - Discover
              - American Express
          SendAutoEmail:
            type: string
            enum: ["Yes", "No"]
            
            
  Pay_Issue_Success_Response:
    type: object
    required:
      - RequestStatus
      - Policy
    properties:
      messageDetail:
        type: object
      RequestStatus:
        type: string
      InsuredName:
        type: string
      QuoteNumber:
        type: string
      Policy:
        type: object
        properties:
          PaymentStatus:
            type: string
          PolicyEffectiveDate:
            type: string
          Issued Date:
            type: string
          PolicyExpirationDate:
            type: string
          PolicyNumber:
            type: string
          ClassType:
            type: string
          StateName:
            type: string
          StateCode:
            type: string
          TRIAPremium:
            type: string
          TotalTFSPremium:
            type: string
          TotalPolicyPremium:
            type: string
          Documents:
            type: array
            items:
              properties:
                DocumentType:
                  type: string
                DocumentName:
                  type: string
                DocumentURL:
                  type: string
                  
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string
