        type: string
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        required:
          - OwnerId
          - RegionCode
          - Token
          - ServiceRequestVersion
          - ServiceResponseVersion
          - ResponseType
        properties:
          ServiceRequestVersion:
            type: string
            default: '1.0'
          ServiceResponseVersion:
            type: string
            default: '1.0'
          OwnerId:
            type: string
            default: '15'
            enum:
              - '15'
          ResponseType:
            type: string
            enum:
              - JSON
              - XML
            default: JSON
          RegionCode:
            type: string
            default: US
          Token:
            type: string
            default: >-
              
          UserName:
            type: string
            default: starragent
          LanguageCode:
            type: string
            default: en
          BrowserIp:
            type: string
      PolicyInformation:
        type: object
        required:
          - ProducerCode
          - ProductVerID
          - ProductID
          - ProductNumber
          - ProductVerNumber
          - OwnerId
        properties:
          ProductVerID:
            type: string
            default: '706'
          ProductID:
            type: string
            default: '619'
          ProductNumber:
            type: string
            default: ILT
          ProductVerNumber:
            type: string
            default: '1.0'
          ProducerCode:
            type: string
            default: '1001'
          MGACode:
            type: string
            default: "1001"
          OwnerId:
            type: string
            default: '15'
            enum:
              - '15'
          CustomerNumber:
            type: string
          RoleID:
            type: string
            default: '5'
          RoleName:
            type: string
            default: Agent
          RoleType:
            type: string
            default: User
          EventName:
            type: string
            default: Pay_IssueV4
            enum:
              - Pay_IssueV4
          CardNumber:
            type: string
            enum:
              - '5555555555554444'
              - '4012888888881881'
              - '6011000990139424'
              - '3530111333300000'
              - '347712669659320'
          CVV:
            type: string
            default: '123'
          ExpiryMonth:
            type: string
          ExpiryYear:
            type: string
          PayerName:
            type: string
          PayerAddress1:
            type: string
          PayerAddress2:
            type: string
          PayerCity:
            type: string
          PayerState:
            type: string
            description: Provide Payer State Code
          PayerCountry:
            type: string
            default: US
