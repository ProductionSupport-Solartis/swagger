swagger: '2.0'
info:
  version: "1.0"
  title: Solartis Policy Issuance API for Embroker
  description: To get rate for Management Liability from Solartis API
  
host: embroker.alpha.solartis.net
basePath: /KnowledgeEngineV6_2/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Issue Policy for the given input
      operationId: issuePolicy
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName  
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string 
          default: "EmbrokerIssuePolicy"
        - name: EventVersion
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "1.0.0.1"
        - name: Policy
          in: body
          description: Policy content
          required: true
          schema:
            $ref: '#/definitions/Policy_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Policy_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
            
definitions:
  Policy_Request:
    type: object
    required:
      - ServiceRequestDetail
      - Policy
    properties:
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "36"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          UserName:
            type: string
            default: "EmbrokerAgent"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          LanguageCode:
            type: string
            default: "en"
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      Policy:
        type: object
        required:
          - QuoteNumber
          - SurplusLinesDisclosureName
          - SurplusLinesDisclosureDate
          - EventVersion
          - SurplusLinesDisclosureTitle
          - WarrantyStatementName
          - WarrantyStatementDate
          - WarrantyStatementTitle
        
        properties:
          ProductNumber:
            title: "Product Number"
            type: string
            default: "EMBRKR"
          ProductVerNumber:
            title: "Product Ver Number"
            type: string
            default: "EMBRKR_2018_V1"
          EventName:
            title: "Event Name"
            type: string
            default: "EmbrokerIssuePolicy"
          EventVersion:
            title: "Event Version"
            type: string
            default: "1.0.0.1"
          QuoteNumber:
            title: "Quote Number"
            type: string
          SurplusLinesDisclosureName:
            title: "Surplus Lines Disclosure Name"
            type: string
          SurplusLinesDisclosureDate:
            title: "Surplus Lines Disclosure Date"
            type: string
          SurplusLinesDisclosureTitle:
            title: "Surplus Lines Disclosure Title"
            type: string
          WarrantyStatementName:
            title: "Warranty Statement Name"
            type: string
          WarrantyStatementDate:
            title: "Warranty Statement Date"
            type: string
          WarrantyStatementTitle:
            title: "Warranty Statement Title"
            type: string
          OutputTypeList:
            type: array
            items:
              properties:
                Type:
                  title: "Type"
                  type: string
                  enum: ["Policy", "Binder"]
      OwnerId:
        title: "Owner ID"
        type: string
        default: "36"    
        
  Policy_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
      - Policy
    properties:
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"
      EndClientUserBrowserSessionId:
        type: string
        default: ""
      EndClientUserBrowserInformation:
        type: string
        default: ""
      PaaSClientIPAddress:
        type: string
        default: ""
      ClientUniqueRequestID:
        type: string
        default: ""
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "36"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          UserName:
            type: string
            default: "EmbrokerAgent"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          LanguageCode:
            type: string
            default: "en"
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      Policy:
        type: object
        properties:
          ProductNumber:
            title: "Product Number"
            type: string
          ProductVerNumber:
            title: "Product Ver Number"
            type: string
          EventName:
            title: "Event Name"
            type: string
          EventVersion:
            title: "Event Version"
            type: string
          PolicyNumber:
            title: "Policy Number"
            type: string
          QuoteNumber:
            title: "Quote Number"
            type: string
          SubmissionNumber:
            title: "Submission Number"
            type: string
          EffectiveDate:
            title: "Effective Date"
            type: string
          ExpirationDate:
            title: "Expiration Date"
            type: string
          TotalDiscountPercent:
            title: "Total Discount Percent"
            type: string
          TotalDiscountFactor:
            title: "Total Discount Percent"
            type: string
          TotalBasePremium:
            title: "Total Base Premium"
            type: string
          AnnualPremium:
            title: "Annual Premium"
            type: string
          ProrataFactor:
            title: "Prorata Factor"
            type: string
          ProratedPremium:
            title: "Prorata Premium"
            type: string
          FinalPremium:
            title: "Prorata Premium"
            type: string
          TotalTaxes:
            title: "Total Taxes"
            type: string
          TotalFees:
            title: "Total Fees"
            type: string
          TaxesFeesAndSurcharges:
            title: "Taxes Fees And Surcharges"
            type: string
          TotalPremium:
            title: "Total Premium"
            type: string
          Commission:
            title: "Commission"
            type: string
          SurplusLinesDisclosureName:
            title: "Surplus Lines Disclosure Name"
            type: string
          SurplusLinesDisclosureDate:
            title: "Surplus Lines Disclosure Date"
            type: string
          SurplusLinesDisclosureTitle:
            title: "Surplus Lines Disclosure Title"
            type: string
          WarrantyStatementName:
            title: "Warranty Statement Name"
            type: string
          WarrantyStatementDate:
            title: "Warranty Statement Date"
            type: string
          WarrantyStatementTitle:
            title: "Warranty Statement Title"
            type: string
          OutputTypeList:
            type: array
            items:
              properties:
                Type:
                  title: "Type"
                  type: string
          Coverage:
            type: array
            items:
              properties:
                CoverageDetail:
                  type: object
                  properties:
                    CoverageName:
                      title: "Coverage Name"
                      type: string
                    LimitOfLiability:
                      title: "Limit Of Liability"
                      type: string
                    Retention:
                      title: "Retention"
                      type: string
                    BaseRate:
                      title: "Base Rate"
                      type: string
      OwnerId:
        title: "Owner ID"
        type: string
        default: "36"
                
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string      
