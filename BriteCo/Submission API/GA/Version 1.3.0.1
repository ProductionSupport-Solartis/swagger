swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis Submission API for BriteCo
  
  
host: briteco.ga.solartis.net
basePath: /KnowledgeEngineV6_4/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Create Submission for the given input
      operationId: createsubmission
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "BRITECOSubmission"
        - name: EventVersion
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "1.3.0.1"
        - name: Submission
          in: body
          description: Submission content
          required: true
          schema:
            $ref: '#/definitions/Submission_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Submission_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  Submission_Request:
    type: object
    required:
      - ServiceRequestDetail
      - Policy
    properties:
      Policy:
        type: object
        properties:
          ProductNumber:
            title: "Product Number"
            type: string
            default: "BRT_IM3"
          ProductVerNumber:
            title: "Product Ver Number"
            type: string
            default: "BRT_IM3_V1"
          EventName:
            title: "Event Name"
            type: string
            default: "BRITECOSubmission"
          EventVersion:
            title: "Event Version"
            type: string
            default: "1.3.0.1"
      InsuredInformation:
        type: object
        properties:
          CustomerID:
            title: "Customer ID"
            type: string
          FirstName:
            title: "First Name"
            type: string
            default: "Moses"
          LastName:
            title: "Last Name"
            type: string
            default: "J"
          PhoneNumber:
            title: "Phone Number"
            type: string
          EmailAddress:
            title: "Email Address"
            type: string
            default: "mosesjebarajp@gmail.com"
          AddressLine1:
            title: "Address Line 1"
            type: string
            default: "826 Goose Hollow Rd"
          AddressLine2:
            title: "Address Line 2"
            type: string
          City:
            title: "City"
            type: string
            default: "Fountain Hill"
          State:
            title: "State"
            type: string
            default: "AR"
            enum: ["AL","AK","AR","AZ","CA","CO","CT","DC","DE","FL","GA","HI","IA","ID","IL","IN","KS","KY","LA","MA","MD","ME","MI","MN","MO","MS","MT","NC","ND","NE","NH","NJ","NM","NV","NY","OH","OK","OR","PA","RI","SC","SD","TN","TX","UT","VT","WA","WI","WV","WY","VA"]
          ZipCode:
            title: "Zip Code"
            type: string
            default: "71642"
          ConsentToSMS:
            title: "Consent To SMS"
            type: string
            enum: ["Yes","No"]
      JewelerInformation:
        type: object
        properties:
          JewelerOrgID:
            title: "Jeweler Org ID"
            type: string
          JewelerOrgCompanyName:
            title: "Jeweler Org Company Name"
            type: string
          JewelerStoreID:
            title: "Jeweler Store ID"
            type: string
          JewelerStoreCompanyName:
            title: "Jeweler Store Company Name"
            type: string
      JewelList:
        type: array 
        items:
          properties:
            JewelDetail:
              type: object
              properties: 
                AppraisalID:
                  title: "Appraisal ID"
                  type: string
                AppraisalDate:
                  title: "Appraisal Date"
                  type: string
                ShortDescription:
                  title: "Short Description"
                  type: string
                AppraisalValue:
                  title: "Appraisal Value"
                  type: string
                AgreedValueFlag:
                  title: "Agreed Value Flag"
                  type: string
                  enum: ["Yes","No"]
                CustomFlag:
                  title: "Custom Flag"
                  type: string
                  enum: ["Yes","No"]
                Type:
                  title: "Type"
                  type: string
      OwnerId:
        type: string
        default: "41"
      ServiceRequestDetail:
        type: object
        properties:
          ServiceRequestVersion:
            type: string
            default: "1.0"
          LanguageCode:
            type: string
            default: "en"
          UserName:
            type: string
            default: "BriteCoagent"
          OwnerId:
            type: string
            default: "41"
          RegionCode:
            type: string
            default: "US"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          ResponseType:
            type: string
            default: "json"
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"

  Submission_Success_Response:
    type: object
