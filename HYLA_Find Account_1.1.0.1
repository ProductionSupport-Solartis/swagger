swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis FindAccount API for HYLA
  description: To Find account details
  
  
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
