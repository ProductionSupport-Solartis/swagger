swagger: '2.0'
info:
  version: '2.0'
  title: Individual Leisure Travel - Issue Policy API
  description: |
    Issue Policy for the Travel plans. 
    
    New Features:
    1. Added CFAR Premium even if CFAR is not opted
    2. Similary added TripCancellation Premium even if CFAR is opted
    3. Added coverage information to the response
  
    Features:
    1. Accept input as JSON request with Plan information
    2. Respond back with premium information
    3. Respond with erorr message incase of invalid request or failure scenarios
    4. Added coverage information along with response
    5. Enhaced Rate API to respond with proper error messages
    
  contact:
    name: Solartis Support
    email: info@solartis.com
    
host: sademokb.uat.solartis.net
basePath: /DroolsV4_2/DroolsService
schemes:
  - https
consumes:
  - application/json
  - application/xml
produces:
  - application/json
  - application/xml
paths:
  /FireEvent:
    post:
      description: This API issues the Policy from the CRN
      operationId: Pay_Issue
      produces:
        - application/json
        - application/xml
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: RequestModel
          in: body
          description: Pay_Issue/insured details
          required: true
          schema:
            $ref: '#/definitions/RequestModel'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/ResponseModel'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
definitions:
  RequestModel:
    type: object
    required:
     - ServiceRequestDetail
    properties:
      ServiceRequestDetail:
        type: object
        required:
          - OwnerId
          - RegionCode
          - Token
          - ServiceRequestVersion
          - ServiceResponseVersion
          - ResponseType
        properties:
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          OwnerId:
            type: string
            default: "24"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          RegionCode:
            type: string
            default: "US"
          Token:
            type: string
            default: ""
          UserName:
            type: string
            default: "BOPAgent"
          LanguageCode:
            type: string
            default: "en"
          BrowserIp:
            type: string
      RepeatedAttributeListDetail:
            type: array
            items:
              properties:
                Key:
                  type: string
                AttributeDetailList:
                  type: array
                  items:
                    properties:
                      AttributeDetail:
                        type: array
                        items:
                          properties:
                            Key:
                              type: string
                            Value:
                              type: string 
      AttributeDetailList:
                type: array
                items:
                  properties:
                    AttributeDetail:
                      type: array
                      items:
                        properties:
                          Key:
                            type: string
                          Value:
                            type: string                      
  ResponseModel:
    type: object
    required:
      - ResponseStatus
    properties:
      ResponseStatus:
        type: string
      attributeListDetail:
        type: array
        items:
          properties:
            AttributeDetail:
              type: array
              items:
                properties:
                  Key:
                    type: string
                  Value:
                    type: string
            repeatedAttributeListDetail:
              type: array
              items:
                properties:
                  Key:
                    type: string
                  AttributeDetailList:
                    type: array
                    items:
                      properties:
                        AttributeDetail:
                          type: array
                          items:
                            properties:
                              Key:
                                type: string
                              Value:
                                type: string
  errorModel:
    type: object
    required:
      - ResponseStatus
      - MessageDetail
    properties:
      ResponseStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string
