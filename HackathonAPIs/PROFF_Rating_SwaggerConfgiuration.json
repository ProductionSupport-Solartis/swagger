{
	"swagger": "2.0",
	"info": {
		"version": "1.0.0",
		"title": "Solartis Rate API for Professional Liability",
		"description": "To get rate for Professional Liability from Solartis Rate API."
	},
	"host": "profapihk.solartis.net",
	"basePath": "/DroolsV6/DroolsService",
	"schemes": [
		"https"
	],
	"produces": [
		"application/json"
	],
	"consumes": [
		"application/json"
	],
	"paths": {
		"/FireEventV2": {
			"post": {
				"description": "Gets Rate for Professional Liability",
				"operationId": "getRates",
				"produces": [
					"application/json"
				],
				"parameters": [
					{
						"name": "Token",
						"in": "header",
						"description": "Authentication Token",
						"required": true,
						"type": "string",
						"format": "string",
						"default": ""
					},
					{
						"name": "Content-Type",
						"in": "header",
						"description": "Content Type",
						"required": true,
						"type": "string",
						"format": "string",
						"default": "application/json"
					},
					{
						"name": "EventName",
						"in": "header",
						"description": "EventName",
						"required": true,
						"type": "string",
						"format": "string",
						"default": "PLICalculateRate"
					},
					{
						"name": "EventVersion",
						"in": "header",
						"description": "EventVersion",
						"required": true,
						"type": "string",
						"format": "string",
						"default": "1.1.1.3"
					},
					{
						"name": "Rating Request",
						"in": "body",
						"description": "For rating request",
						"required": true,
						"schema": {
							"$ref": "#/definitions/Rating_Request"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Successful response",
						"schema": {
							"$ref": "#/definitions/Rating_Success_Response"
						}
					},
					"default": {
						"description": "unexpected error",
						"schema": {
							"$ref": "#/definitions/errorModel"
						}
					}
				}
			}
		}
	},
	"definitions": {
		"Rating_Request": {
			"type": "object",
			"properties": {
				"EndClientUserUniqueSessionId": {
					"type": "string",
					"default": "Uniquesession"
				},
			  	"ServiceRequestDetail": {
					"type": "object",
					"properties": {
						"OwnerId": {
							"type": "string",
							"default": "27"
						},
						"ResponseType": {
							"type": "string",
							"enum": [
								"JSON",
								"XML"
							],
							"default": "JSON"
						},
						"UserName": {
							"type": "string",
							"default": "CWAgent"
						},
						"Token": {
							"type": "string",
							"default": ""
						},
						"BrowserIp": {
							"type": "string",
							"default": "127.0.0.1"
						},
						"ServiceRequestVersion": {
							"type": "string",
							"default": "1.0"
						},
						"RegionCode": {
							"type": "string",
							"default": "US"
						}
					}
				},
				"Policy": {
					"type": "object",
					"properties": {
						"PolicyType": {
							"type": "string",
							"default": "Individual"
						},
						"ZipCode": {
							"type": "string",
							"default": "15610"
						},
						"State": {
							"type": "string",
							"default": "PA"
						},
						"EffectiveDate": {
							"type": "string",
							"default": "2017-07-27"
						},
						"EventName": {
							"type": "string",
							"default": "PLICalculateRate"
						},
						"EventVersion": {
							"type": "string",
							"default": "1.1.1.3"
						},
						"ApplicableFromDate": {
							"type": "string",
							"default": "2017-07-01"
						},
						"NumOfProviders": {
							"type": "string",
							"default": "1"
						},
						"SharedOrSeparateLimit": {
							"type": "string",
							"default": "Shared"
						},
						"PLOrWLCoverageType": {
							"type": "string",
							"default": "Occurrence"
						},
						"LiabilityLimitsOfInsurance": {
							"type": "string",
							"default": "$1M/$5M"
						},
						"WorkplaceLiabilityInsurance": {
							"type": "string",
							"default": "Yes"
						},
						"CorporationOrPartnershipCoverage": {
							"type": "string",
							"default": "Yes"
						},
						"TransitionFactor": {
							"type": "string",
							"default": "16"
						},
						"AdditionalInsuredCoverage": {
							"type": "string",
							"default": "Yes"
						},
						"EmploymentCategory": {
							"type": "string",
							"default": "Employed"
						},
						"ProductNumber": {
							"type": "string",
							"default": "HC_PLI"
						},
						"ProductVerNumber": {
							"type": "string",
							"default": "1.0"
						},
				"Provider":{
					"type": "array",
					"items":{
					  "properties": {
						"ProviderDetail": {
							"type": "object",
							"properties": {
							  "ProviderClass": {
								  "type": "string",
								  "default": "Blood Bank Technician/Technologist"
							  },
							  "ProfessionalAssociation": {
								  "type": "string",
								  "default": "Licensed"
							  },
							  "YearsOfPolicy": {
								  "type": "string",
								  "default": "1"
							  },
							  "YearsInPractice": {
								  "type": "string",
								  "default": "1"
							  },
							  "HoursWorkPerWeek": {
								  "type": "string",
								  "default": "15"
							  },
							  "ScheduleRatingFactor": {
								  "type": "string",
								  "default": "15"
							  }
							}
						}	
					}
				}
				}
					}
				},
				"OwnerId":{
					"type": "string",
					"default": "27"
				}
			}
		},
		"Rating_Success_Response": {
			"type": "object",
			"properties": {
				"messageDetail": {
					"type": "object"
				},
				"ResponseStatus": {
					"type": "string"
				},
				"Policy": {
					"type": "object"
				}
			}
		},
		"errorModel": {
			"type": "object",
			"required": [
				"RequestStatus",
				"MessageDetail"
			],
			"properties": {
				"RequestStatus": {
					"type": "string"
				},
				"MessageDetail": {
					"type": "object",
					"properties": {
						"MessageCode": {
							"type": "string"
						},
						"MessageType": {
							"type": "string"
						},
						"UserMessage": {
							"type": "string"
						},
						"MessageLevel": {
							"type": "string"
						},
						"MessageSeverity": {
							"type": "string"
						},
						"MessageList": {
							"type": "object",
							"properties": {
								"Message": {
									"type": "array",
									"items": {
										"type": "string"
									}
								}
							}
						}
					}
				}
			}
		}
	}
}
